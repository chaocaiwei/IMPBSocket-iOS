platform :ios, '10.0'


def shared_pods
    use_frameworks!
    pod 'AFNetworking', '~> 3.1.0'
    pod 'CocoaAsyncSocket', '~> 7.6.1'
    pod 'ProtocolBuffers-Swift', '4.0.1'
    pod 'Alamofire', '~> 4.7.0'
    pod 'Moya', '~> 11.0'
    pod 'SDWebImage', '~> 4.0'
    pod 'boost', '~> 1.59.0'
end

target "IMSocket"  do
    shared_pods
end


target "TodayTest" do
    shared_pods
end
